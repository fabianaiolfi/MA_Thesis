@article{limElectoralConsequencesInternational2023,
  title = {The {{Electoral Consequences}} of {{International Migration}} in {{Sending Countries}}: {{Evidence}} from {{Central}} and {{Eastern Europe}}},
  shorttitle = {The {{Electoral Consequences}} of {{International Migration}} in {{Sending Countries}}},
  author = {Lim, Junghyun},
