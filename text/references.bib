@article{alrababahBacklashThreatAntiimmigrant,
  title = {Backlash without Threat? {{Anti-immigrant}} Voting in the Absence of Cultural, Economic, and Security Concerns},
  author = {Alrababa'h, Ala' and Beerli, Andreas and Hangartner, Dominik and Ward, Dalston},
  abstract = {Prominent explanations of anti-immigrant sentiment and voting give centrality to cultural, economic, and security threats posed by newcomers. We test these predictions in Switzerland, which opened its labor market to neighboring countries in the 2000s. Using a difference-in-differences approach, we show that the number of immigrants in Swiss border municipalities increased dramatically after the borders opened, followed by a more than six percentage point increase in support for anti-immigrant parties. Turning to standard explanations for this backlash, we find no adverse effect on employment and wages, perceptions of economic, cultural, or security threats, or traffic density. Instead, we uncover suggestive evidence that political elites raised hostility toward immigrants and were most successful among voters vulnerable to political persuasion. Overall, our findings contribute to our understanding of how political elites can fuel anti-immigrant sentiment and how the principle of free movement in the European Union can have significant political repercussions.},
  langid = {english},
  file = {/Users/aiolf1/Zotero/storage/ILYTH653/Alrababa’h et al. - Backlash without threat Anti-immigrant voting in .pdf}
}

