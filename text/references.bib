@article{safiSwissEuroskepticismEconomically,
  title = {Swiss {{Euroskepticism}}: {{Economically}} or {{Culturally Determined}}?},
  author = {Safi, Katayoun},
  pages = {20},
  abstract = {Theories that try to explain the debate on Euroskepticism can be narrowed down to two main approaches. The calculus approach considers support or opposition to the EU as a result of economic rationality. The cultural approach, on the other hand, regards Euroskepticism as a consequence of value orientations. Whereas previous studies focused more on the economic determinants of Euroskepticism, recent studies on Euroskepticism started to integrate cultural explanatory variables. The aim of this paper is to identify the causes and consequences of Swiss Euroskepticism in a longitudinal analysis. Distinguishing between the economic and the cultural dimension, I examine to which extent the Swiss Euroskepticism landscape has evolved since the first voting on the EEA entry. The paper shows that Euroskepticism in Switzerland has mainly cultural causes, and is only indirectly linked to socio-economic variables. It finds evidence for the fact that with the growing cultural impact of the EU, slight economic causes were replaces by a growing cultural effect.},
